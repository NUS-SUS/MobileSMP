    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.mobilesmp">
    <queries>
        <intent>
            <action android:name="android.intent.action.VIEW" />
            <data android:scheme="https" />
        </intent>
        <intent>
            <action android:name=
